
@section css {
    <!-- Page-Level Plugin CSS - Tables -->
    @*<link href="~/Content/plugins/datepicker/datepicker.css" rel="stylesheet">
        <link href="~/Content/plugins/dataTables/dataTables.bootstrap.css" rel="stylesheet">*@
}

<div class="row">
    <div class="col-md-12 col-md-offset-0">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title"> @ViewBag.Title </h3>
            </div>
            <div class="panel-body">
                <form role="form" id="frmProyectar" autocomplete="off">
                    <fieldset>

                        <div class="panel panel-default">
                            <div class="panel-footer">Datos Busqueda</div>

                            <div class="panel-body">
                                <div class="panel-body">

                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="panel panel-default">
                                                <div class="panel-body">
                                                    <div class="errorMsgPanel"></div>
                                                    <div class="successMsgPanel"></div>
                                                    @Html.AntiForgeryToken()

                                                    <div class="col-md-1">
                                                        <label for="txtSucursal" class="control-label">Departamento:</label>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <select class="form-control" id="departamento" name="departamento">
                                                            <option value="1">Amazonas</option>
                                                            <option value="2">Áncash</option>
                                                            <option value="3">Apurímac</option>
                                                            <option value="4">Arequipa</option>
                                                            <option value="5">Ayacucho</option>
                                                            <option value="6">Cajamarca</option>
                                                            <option value="7">Callao</option>
                                                            <option value="8">Cusco</option>
                                                            <option value="9">Huancavelica</option>
                                                            <option value="10">Huánuco</option>
                                                            <option value="11">Ica</option>
                                                            <option value="12">Junín</option>
                                                            <option value="13">La Libertad</option>
                                                            <option value="14">Lambayeque</option>
                                                            <option value="15" selected="selected">Lima</option>
                                                            <option value="16">Loreto</option>
                                                            <option value="17">Madre de Dios</option>
                                                            <option value="18">Moquegua</option>
                                                            <option value="19">Pasco</option>
                                                            <option value="20">Piura</option>
                                                            <option value="21">Puno</option>
                                                            <option value="22">San Martín</option>
                                                            <option value="23">Tacna</option>
                                                            <option value="24">Tumbes</option>
                                                            <option value="25">Ucayali</option>
                                                        </select>
                                                    </div>

                                                    @*<div class="col-md-1">
                                                            <label for="txtSucursal" class="control-label">Mes:</label>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <select class="form-control" id="mes" name="mes">
                                                                <option value="01" selected="selected">ENERO</option>
                                                                <option value="02">FEBRERO</option>
                                                                <option value="03">MARZO</option>
                                                                <option value="04">ABRIL</option>
                                                                <option value="05">MAYO</option>
                                                                <option value="06">JUNIO</option>
                                                                <option value="07">JULIO</option>
                                                                <option value="08">AGOSTO</option>
                                                                <option value="09">SEPTIEMBRE</option>
                                                                <option value="10">OCTUBRE</option>
                                                                <option value="11">NOVIEMBRE</option>
                                                                <option value="12">DICIEMBRE</option>
                                                            </select>
                                                        </div>*@

                                                    <div class="col-md-6">
                                                        <button type="button" id="btnBuscar" name="btnBuscar" class="btn btn-outline btn-primary">Buscar</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <iframe id="idFrame" frameborder="0" style="height:1400px; width: 100%;" scrolling="yes"></iframe>

                        <div class="loadingDiv" style="width: 100%;">
                            <img src="@Url.Content("~/Content/images/ajax-loader.gif")" />
                            @Html.AntiForgeryToken()
                        </div>
                    </fieldset>
                </form>
                <br />
                <div id="frmListarProcesosISOContent"></div>
            </div>
        </div>
    </div>
</div>

@section scripts {

    @*<script src="~/Scripts/plugins/datepicker/bootstrap-datepicker.js" charset="UTF-8"></script>
        <script src="~/Scripts/plugins/datepicker/bootstrap-datepicker.es.min.js" charset="UTF-8"></script>*@

    @*<script src="~/Scripts/plugins/dataTables/jquery.dataTables.min.js"></script>
        <script src="~/Scripts/plugins/dataTables/dataTables.bootstrap.js"></script>*@

    <script src="~/Views/ProyectarNecesidadTransporte/MantProyectarNecesidadTransporte.js"></script>
    <script type="text/javascript">
        // Ejecucion de script para vista
        (function () {
            docInit("@Url.Content("~/")");
        })();
    </script>
}


